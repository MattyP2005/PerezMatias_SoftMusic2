            "type": "string"
          },
          "genero": {
            "type": "string",
            "nullable": true
          },
          "url": {
            "minLength": 1,
            "type": "string"
          },
          "fechaSubida": {
            "type": "string",
            "format": "date-time"
          },
          "artistaId": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "CreateUsuarioDTOs": {
        "required": [
